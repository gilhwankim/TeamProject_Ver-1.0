<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" prefHeight="720.0" prefWidth="900.0" spacing="20.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pos.javafile.SalesStatusController">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <children>
      <Label styleClass="font_bold" prefHeight="79.0" prefWidth="80.0" text="매장분석">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <GridPane prefHeight="544.0" prefWidth="880.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="20.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="20.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <VBox>
               <children>
                  <Label styleClass="font_bold" prefHeight="60.0" prefWidth="368.0" text="월 결제 금액">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Label>
                  <Label fx:id="monthlyPay" prefHeight="60.0" prefWidth="464.0" text="0원" />
               </children>
            </VBox>
            <VBox GridPane.columnIndex="1">
               <children>
                  <Label styleClass="font_bold" prefHeight="60.0" prefWidth="368.0" text="   월 결제 건수">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Label>
                  <Label fx:id="monthlyPayCnt" prefHeight="48.0" prefWidth="264.0" text="0건">
                     <VBox.margin>
                        <Insets left="20.0" />
                     </VBox.margin></Label>
               </children>
            </VBox>
            <VBox GridPane.columnIndex="2">
               <children>
                  <Label styleClass="font_bold" prefHeight="60.0" prefWidth="368.0" text="최근 30일 평균 결제 금액">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Label>
                  <Label fx:id="lastThirtydayPay" prefHeight="60.0" prefWidth="464.0" text="0원" />
               </children>
            </VBox>
            <VBox GridPane.rowIndex="1">
               <children>
                  <Label styleClass="font_bold" prefHeight="60.0" prefWidth="368.0" text="일 결제 금액">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Label>
                  <Label fx:id="dailyPay" prefHeight="60.0" prefWidth="464.0" text="0원" />
               </children>
            </VBox>
            <VBox GridPane.columnIndex="1" GridPane.rowIndex="1">
               <children>
                  <Label styleClass="font_bold" prefHeight="60.0" prefWidth="368.0" text="   일 결제 건 수">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Label>
                  <Label fx:id="dailyPayCnt" prefHeight="60.0" prefWidth="264.0" text="0원">
                     <VBox.margin>
                        <Insets left="20.0" />
                     </VBox.margin></Label>
               </children>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="100.0" GridPane.rowIndex="2" GridPane.rowSpan="2">
               <children>
                  <Label styleClass="font_bold" prefHeight="60.0" prefWidth="368.0" text="결제 수단 별 월 매출">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Label>
                  <HBox prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="47.0" prefWidth="90.0" text="카드" />
                        <Label fx:id="cardPay" alignment="CENTER_RIGHT" contentDisplay="CENTER" prefHeight="47.0" prefWidth="207.0" text="0원" />
                     </children>
                  </HBox>
                  <ProgressBar fx:id="cardBar" prefHeight="18.0" prefWidth="304.0" progress="0.0" />
                  <HBox prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="47.0" prefWidth="90.0" text="현금" />
                        <Label fx:id="cashPay" alignment="CENTER_RIGHT" contentDisplay="CENTER" prefHeight="47.0" prefWidth="207.0" text="0원" />
                     </children>
                  </HBox>
                  <ProgressBar fx:id="cashBar" prefHeight="18.0" prefWidth="304.0" progress="0.0" />
               </children>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="2" GridPane.rowSpan="2">
               <children>
                  <Label styleClass="font_bold" prefHeight="60.0" prefWidth="312.0" text="                               판매량 순위">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Label>
                  <PieChart fx:id="bestMenu" prefHeight="296.0" prefWidth="586.0" />
               </children>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="2" GridPane.rowIndex="1">
               <children>
                  <Label styleClass="font_bold" prefHeight="48.0" prefWidth="368.0" text="날짜 선택">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Label>
                  <DatePicker fx:id="dateSel" />
               </children>
            </VBox>
         </children>
      </GridPane>
   </children>
</VBox>