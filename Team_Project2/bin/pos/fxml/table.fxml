<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.*?>
<?import com.jfoenix.controls.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="TOP_CENTER" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox id="vb" alignment="TOP_CENTER" prefHeight="118.0" prefWidth="287.0" VBox.vgrow="ALWAYS">
         <children>
            <HBox alignment="CENTER" fillHeight="false">
               <children>
                  <Circle id="circle" fill="RED" radius="6.0" stroke="BLACK" strokeType="INSIDE">
                     <HBox.margin>
                        <Insets left="5.0" />
                     </HBox.margin>
                  </Circle>
                  <HBox alignment="CENTER" fillHeight="false" HBox.hgrow="ALWAYS">
                     <children>
                        <Label id="tableNo" prefHeight="15.0" text="테이블 번호 : " HBox.hgrow="ALWAYS" />
                        <TextField id="tf" editable="false" prefWidth="40.0" />
                     </children>
                  </HBox>
               </children>
               <VBox.margin>
                  <Insets bottom="3.0" />
               </VBox.margin>
            </HBox>
            <ListView id="lv" maxWidth="200.0" prefHeight="77.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
            <HBox>
               <children>
                  <HBox alignment="TOP_RIGHT" HBox.hgrow="ALWAYS">
                     <children>
                        <Label id="price" alignment="TOP_RIGHT" contentDisplay="RIGHT" prefHeight="18.0" prefWidth="112.0" text="가격">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </Label>
                     </children>
                     <padding>
                        <Insets right="5.0" />
                     </padding>
                  </HBox>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</VBox>
